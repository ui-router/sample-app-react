{
  "name": "@uirouter/sample-app-react",
  "version": "1.0.0",
  "description": "Architecture overview demo for UI-Router React",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --host 0.0.0.0 --port 3000 --config webpack.config.js --history-api-fallback --content-base ./",
    "build": "rimraf docs && mkdirp docs && cp index.html docs/index.html && NODE_ENV=production webpack",
    "test": "run-s build e2e",
    "test:ui": "run-s build && run-p -r browser-sync cypress:ui",
    "e2e": "run-p -r browser-sync cypress",
    "cypress": "wait-on tcp:4000 && cypress run",
    "cypress:ui": "wait-on tcp:4000 && cypress open",
    "browser-sync": "browser-sync start --port 4000 --server docs --no-open",
    "gh-pages": "npm run build && git add docs && git commit -m 'update github pages' docs && git push"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@uirouter/react": "0.6.2",
    "@uirouter/visualizer": "6.0.0",
    "lodash": "4.17.5",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "8.0.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-syntax-trailing-function-commas": "6.22.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2016": "6.24.1",
    "babel-preset-react": "6.24.1",
    "browser-sync": "2.23.6",
    "copy-webpack-plugin": "4.4.1",
    "css-loader": "0.28.9",
    "cypress": "2.0.2",
    "file-loader": "1.1.7",
    "npm-run-all": "4.1.2",
    "postcss-load-config": "1.2.0",
    "postcss-loader": "2.1.0",
    "rimraf": "2.6.2",
    "style-loader": "0.20.2",
    "wait-on": "2.1.0",
    "webpack": "3.11.0",
    "webpack-dev-server": "2.11.1"
  }
}
